{
    "name": "keep-grass-vscode",
    "displayName": "keep.grass",
    "description": "This vscode extion shows the last commit timestamp in order to keep your github active. ( keep.grass for vscode )",
    "version": "0.0.1",
    "publisher": "wraith13",
    "license": "SEE LICENSE IN LICENSE_1_0.txt",
    "engines": {
        "vscode": "^1.5.0"
    },
    "bugs": {
        "url": "https://github.com/wraith13/keep-grass-vscode/issues"
    },
    "homepage": "https://github.com/wraith13/keep-grass-vscode/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/wraith13/keep-grass-vscode.git"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "keep-grass.update",
                "title": "update",
                "category": "keep.grass"
            }
        ],
        "configuration": {
            "title": "keep.grass configuration",
            "properties": {
                "keep-grass-vscode.user": {
                    "type": "string",
                    "default": null,
                    "description": "Your GitHub account"
                },
                "keep-grass-vscode.show": {
                    "type": "string",
                    "default": "both",
                    "description": "Select show item on status bar",
                    "enum": [
                        "left time",
                        "last stamp",
                        "both"
                    ]
                }
            }
        }
    },    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "dependencies": {
        "request": "^2.79.0"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}